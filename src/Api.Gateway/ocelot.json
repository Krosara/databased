{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/assets",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "9001"
        }
      ],
      "UpstreamPathTemplate": "/gateway/assets",
      "UpstreamHttpMethod": ["POST", "PUT", "GET"],
      "RateLimitOptions": {
        "ClientWhitelist": [],
        "EnableRateLimiting": true,
        "Period": "5s",
        "PeriodTimespan": 1,
        "Limit": 3,
        "HttpStatusCode": 429
      },
      "FileCackeOptions": {
        "TtlSeconds": 30,
        "Region": "assetcache"
      },
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer",
        "AllowedScopes": ["read:assets"]
      }
    },
    {
      "DownstreamPathTemplate": "/api/assets/{id}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "9001"
        }
      ],
      "UpstreamPathTemplate": "/gateway/assets/{id}",
      "UpstreamHttpMethod": ["GET", "DELETE"],
      "RateLimitOptions": {
        "ClientWhitelist": [],
        "EnableRateLimiting": true,
        "Period": "5s",
        "PeriodTimespan": 1,
        "Limit": 3,
        "HttpStatusCode": 429
      },
      "FileCackeOptions": {
        "TtlSeconds": 30,
        "Region": "assetcache"
      }
    }
  ]
}
